        "prefix": "dject-vue-script",
        "body": [
            "const dependencies = [",
            "    '__container'",
            "]",
            "",
            "function $1 (...injectedDependencies) {",
            "    const [container] = injectedDependencies;",
            "    const {} = container.buildDependencyMap(dependencies, injectedDependencies);",
            "",
            "    $3",
